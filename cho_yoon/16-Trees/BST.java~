public class BST{

    private Node root;

    public BST(){
	root = null;
    }

    public Node insert(Node n, int x){
	if (root = null)
	    root = new Node(x);
	else if (root.getData() > x)
	    insert(root.getLeft(), x);
	else 
	    insert(root.getRight(), x);
    }

    public Node Search(int x){
	Node n = root;
	while (n != null){
	    if (n.getData() == x)
		return n;
	    else if (x < n.getData())
		n = n.getLeft();
	    else
		n = n.getRight();
	}
	return null;
    }

