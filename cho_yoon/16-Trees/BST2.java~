public class BST{

    private Node root;

    public BST(){
	root = null;
    }

    public Node insert(Node n, int x){
	if (root = null)
	    root = new Node(x);
	else if (root.getData() > x)
	    insert(root.getLeft(), x);
	else 
	    insert(root.getRight(), x);
    }

    public Node Search(int x){
	Node n = root;
	while (n != null){
	    if (n.getData() == x)
		return n;
	    else if (x < n.getData())
		n = n.getLeft();
	    else
		n = n.getRight();
	}
	return null;
    }

    public Node search2(Node c, int x){
	if (node = null || node.getData() == x)
	    return node;
	else if (x < node.getData())
	    return search2(node.left, x);
	else
	    return search2(node.right, x);
    }

    public static void main(String[] args){
	
    }

}
